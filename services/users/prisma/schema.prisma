generator client {
  provider = "prisma-client-js"
   binaryTargets = ["native", "darwin", "darwin-arm64", "linux-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String @id @default(uuid())
  email       String @unique
  firstName   String
  lastName    String
  role        String
}